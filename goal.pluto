class Animal {
    func constructor(name) {
        this.name = name;
    }
}

class Dog extends Animal {
    func constructor(name) {
        this.name = name;
    }

    func bark() {
        Pluto.System.writeLine(Pluto.String.valueOf(this.name) + " barked");
    }
}

var myDog = Dog("myDog");
myDog.bark(); // myDog barked